  <script type="text/javascript">
  $(function(){
      $("#wrap_tab .list_nav ul li").hover
      (function(){
          var _index=0;
          $(this).addClass("active").siblings().removeClass();
          _index=$(this).index();
          $(".new_con").eq(_index).show().siblings().hide();
      },function(){ }   
      )
  })
